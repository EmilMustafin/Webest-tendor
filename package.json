{
  "name": "webest-tendor",
  "version": "1.0.0",
  "description": "tendors for web development",
  "type": "module",
  "keywords": [
    "nodejs",
    "npm",
    "vue",
    "eslint",
    "stylelint"
  ],
  "author": {
    "name": "Emil"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build ",
    "test:lint": "npm run test:lint:eslint && npm run test:lint:style",
    "test:lint:eslint": "eslint --ext .vue,.ts,.tsx,.js,.jsx,.cjs,.mjs,.json,.yml,.yaml --max-warnings 0 .",
    "test:lint:style": "stylelint --max-warnings 0 --ignore-path .gitignore \"**/*.{css,scss,vue,vuex}\""
  },
  "dependencies": {
    "vue": "3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
